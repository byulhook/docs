#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/love1ace/Library/Mobile Documents/com~apple~CloudDocs/Projects/byuldocs/docs/node_modules/.pnpm/byulhook@0.1.7/node_modules/byulhook/dist/bin/node_modules:/Users/love1ace/Library/Mobile Documents/com~apple~CloudDocs/Projects/byuldocs/docs/node_modules/.pnpm/byulhook@0.1.7/node_modules/byulhook/dist/node_modules:/Users/love1ace/Library/Mobile Documents/com~apple~CloudDocs/Projects/byuldocs/docs/node_modules/.pnpm/byulhook@0.1.7/node_modules/byulhook/node_modules:/Users/love1ace/Library/Mobile Documents/com~apple~CloudDocs/Projects/byuldocs/docs/node_modules/.pnpm/byulhook@0.1.7/node_modules:/Users/love1ace/Library/Mobile Documents/com~apple~CloudDocs/Projects/byuldocs/docs/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/love1ace/Library/Mobile Documents/com~apple~CloudDocs/Projects/byuldocs/docs/node_modules/.pnpm/byulhook@0.1.7/node_modules/byulhook/dist/bin/node_modules:/Users/love1ace/Library/Mobile Documents/com~apple~CloudDocs/Projects/byuldocs/docs/node_modules/.pnpm/byulhook@0.1.7/node_modules/byulhook/dist/node_modules:/Users/love1ace/Library/Mobile Documents/com~apple~CloudDocs/Projects/byuldocs/docs/node_modules/.pnpm/byulhook@0.1.7/node_modules/byulhook/node_modules:/Users/love1ace/Library/Mobile Documents/com~apple~CloudDocs/Projects/byuldocs/docs/node_modules/.pnpm/byulhook@0.1.7/node_modules:/Users/love1ace/Library/Mobile Documents/com~apple~CloudDocs/Projects/byuldocs/docs/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../dist/bin/byulhook.js" "$@"
else
  exec node  "$basedir/../../dist/bin/byulhook.js" "$@"
fi
